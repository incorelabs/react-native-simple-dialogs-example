{
    "expo": {
        "name": "react-native-simple-dialogs-example",
        "description": "This is an Expo example project for the react-native-simple-dialogs plugin for React Native",
        "slug": "react-native-simple-dialogs-example",
        "privacy": "public",
        "sdkVersion": "30.0.0",
        "platforms": ["ios", "android"],
        "version": "0.1.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        }
    }
}